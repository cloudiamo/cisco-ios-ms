  <command name="CREATE">
    <operation><![CDATA[{if $params.type == 'ipv4' || $params.type == 'fqdn' || $params.type == 'ipv6'}
	{if $params.type == 'ipv4'}
		ip access-list extended whitelist_ipv4
	{elseif $params.type == 'fqdn'}
		ip access-list extended whitelist_fqdn
	{elseif $params.type == 'ipv6'}
		ipv6 access-list whitelist_ipv6
	{/if}

	{foreach $params.destinations as $item}
		{if $params.type == 'ipv4'}
			permit ip any object-group {$item.destination}
		{elseif $params.type == 'fqdn'}
			permit ip any fqdn-group {$item.destination}
		{elseif $params.type == 'ipv6'}
			permit ipv6 any object-group {$item.destination}
		{/if}
	{/foreach}
{/if}]]></operation>
  </command>
  <command name="READ"/>
  <command name="UPDATE">
    <operation><![CDATA[{if $params.type == 'ipv4' || $params.type == 'fqdn' || $params.type == 'ipv6'}
  {assign var="acl_header" value=""}
  {assign var="permit_cmd" value=""}
  {assign var="no_permit_cmd" value=""}
  {if $params.type == 'ipv4'}
    {assign var="acl_header" value="ip access-list extended whitelist_ipv4"}
    {assign var="permit_cmd" value="permit ip any object-group"}
    {assign var="no_permit_cmd" value="no permit ip any object-group"}
  {elseif $params.type == 'fqdn'}
    {assign var="acl_header" value="ip access-list extended whitelist_fqdn"}
    {assign var="permit_cmd" value="permit ip any fqdn-group"}
    {assign var="no_permit_cmd" value="no permit ip any fqdn-group"}
  {elseif $params.type == 'ipv6'}
    {assign var="acl_header" value="ipv6 access-list whitelist_ipv6"}
    {assign var="permit_cmd" value="permit ipv6 any object-group"}
    {assign var="no_permit_cmd" value="no permit ipv6 any object-group"}
  {/if}
  {$acl_header}
  {foreach $params.destinations as $item}
    {$permit_cmd} {$item.destination|trim}
  {/foreach}
{foreach $FirewallPolicy.{$params.object_id}.destinations as $old_item}
{assign var="found" value=false}
{foreach $params.destinations as $new_item}
{if $new_item.destination|trim == $old_item.destination|trim}
{assign var="found" value=true}
{break}
{/if}
{/foreach}
{if !$found}
{$no_permit_cmd} {$old_item.destination|trim}
{/if}
{/foreach}
{/if}]]></operation>
  </command>
