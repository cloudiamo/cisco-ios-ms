  <command name="CREATE">
    <operation><![CDATA[{if $params.type == 'ipv4'}
	ip access-list extended whitelist_ipv4
	{foreach $params.destinations as $item}
		permit ip any object-group {$item.destination}
	{/foreach}
{elseif $params.type == 'fqdn'}
	ip access-list extended whitelist_fqdn
	{foreach $params.destinations as $item}
		permit ip any fqdn-group {$item.destination}
	{/foreach}
{elseif $params.type == 'ipv6'}
	ipv6 access-list whitelist_ipv6
	{foreach $params.destinations as $item}
		permit ipv6 any object-group {$item.destination}
	{/foreach}
{/if}]]></operation>
  </command>
  <command name="READ"/>
  <command name="UPDATE">
    <operation><![CDATA[{if $params.type == 'grp_ipv4' || $params.type == 'grp_fqdn' || $params.type == 'grp_ipv6'}
	{if $params.type == 'grp_ipv4'}
		ip access-list extended {$params.object_id}
	{elseif $params.type == 'grp_fqdn'}
		ip access-list extended whitelist_fqdn
	{elseif $params.type == 'grp_ipv6'}
		ipv6 access-list whitelist_ipv6
	{/if}

	{foreach $params.destinations as $item}
		{if $params.type == 'grp_ipv4'}
			permit ip any object-group {$item.destination}
		{elseif $params.type == 'grp_fqdn'}
			permit ip any fqdn-group {$item.destination}
		{elseif $params.type == 'grp_ipv6'}
			permit ipv6 any object-group {$item.destination}
		{/if}
	{/foreach}

	{foreach $FirewallPolicy.{$params.object_id}.destinations as $old_item}
		{assign var="found" value=false}
		{foreach $params.destinations as $new_item}
			{if $new_item.destination == $old_item.destination}
				{assign var="found" value=true}
			{/if}
		{/foreach}
		{if !$found}
			{if $params.type == 'grp_ipv4' || $params.type == 'grp_fqdn'}
				no permit ip any object-group {$old_item.destination}
			{elseif $params.type == 'grp_ipv6'}
				no permit ipv6 any object-group {$old_item.destination}
			{/if}
		{/if}
	{/foreach}
{/if}]]></operation>
  </command>
  <command name="CONSTRAINT"/>
  <command name="DELETE">
    <operation><![CDATA[{if $params.type == 'ipv4'}
	no ip access-list extended whitelist_ipv4
{elseif $params.type == 'fqdn'}
	no ip access-list extended whitelist_fqdn
{elseif $params.type == 'ipv6'}
	no ipv6 access-list whitelist_ipv6
{/if}]]></operation>
